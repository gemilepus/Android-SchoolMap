<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vine.projectdemo">
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:name="com.vine.projectdemo.Values.GlobalVariable"
        android:icon="@drawable/baseline_my_location"
        android:label="@string/app_name"
        android:roundIcon="@drawable/baseline_my_location"
        android:supportsRtl="true"
        android:networkSecurityConfig="@xml/network_security_config"
        android:theme="@style/AppTheme">

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:theme="@style/AppTheme.NoActionBar"
            android:name="com.vine.projectdemo.AccountView.PHPMainActivity"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:theme="@style/AppTheme.NoActionBar"
            android:name="com.vine.projectdemo.DataView.JSONMainActivity"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:theme="@style/AppTheme.NoActionBar"
            android:name="com.vine.projectdemo.View.MapCallMainActivity"
            android:screenOrientation="portrait">
        </activity>

    </application>
</manifest>