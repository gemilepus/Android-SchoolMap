<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vine.projectdemo">
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:name="com.vine.projectdemo.VineReValues.GlobalVariable"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:networkSecurityConfig="@xml/network_security_config"
        android:theme="@style/AppTheme">

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:theme="@style/AppTheme.ActionBar.Transparent"
            android:name="com.vine.projectdemo.VineMapView.MapMainActivity">
        </activity>

        <activity
            android:name="com.vine.projectdemo.ShowValue">
        </activity>

        <activity
            android:theme="@style/AppTheme.NoActionBar"
            android:name="com.vine.projectdemo.VinePHPMySQL.PHPMainActivity"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:theme="@style/AppTheme.NoActionBar"
            android:name="com.vine.projectdemo.VineJsonParsing.JSONMainActivity"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:theme="@style/AppTheme.NoActionBar"
            android:name="com.vine.projectdemo.VineMapView.MapCallMainActivity"
            android:screenOrientation="portrait">
        </activity>

    </application>
</manifest>